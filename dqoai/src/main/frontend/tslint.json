{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "no-empty": false,
    "no-shadowed-variable": false,
    "only-arrow-functions": false,
    "prettier": true,
    "jsx-boolean-value": false
  }
}
