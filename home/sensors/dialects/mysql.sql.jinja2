{% macro date_trunc(value, part='day', datatype=none) %}
    {%- if part == 'year' -%}
        DATE_FORMAT({{value}}, '%Y-01-01 00:00:00')
    {%- elif part == 'quarter' -%}
        DATE_FORMAT(DATE_ADD({{value}}, INTERVAL(QUARTER({{value}})-1) QUARTER), '%Y-%m-01 00:00:00')
    {%- elif part == 'month' -%}
        DATE_FORMAT({{value}}, '%Y-%m-01 00:00:00')
    {%- elif part == 'week' -%}
        DATE_FORMAT(DATE_SUB({{value}}, INTERVAL DAYOFWEEK({{value}})-1 DAY), '%Y-%m-%d 00:00:00')
    {%- elif part == 'day' -%}
        {%- if datatype | upper == 'DATE' -%}
            {{value}}
        {%- else -%}
            DATE({{value}})
        {%- endif -%}
    {%- elif part == 'hour' -%}
        DATE_FORMAT({{value}}, '%Y-%m-%d %H:00:00')
    {%- elif part == 'millisecond' -%}
        {{value}}
    {%- else -%}
        DATE({{value}})
    {%- endif -%}
{% endmacro %}

{% macro date_trunc_utc(value, part='day', datatype=none) %}
    {%- if part == 'year' -%}
        DATE_FORMAT({{value}}, '%Y-01-01 00:00:00')
    {%- elif part == 'quarter' -%}
        DATE_ADD(DATE_FORMAT({{value}}, '%Y-%m-01 00:00:00'), INTERVAL (QUARTER(DATE({{value}}))-1) QUARTER)
    {%- elif part == 'month' -%}
        DATE_FORMAT({{value}}, '%Y-%m-01 00:00:00')
    {%- elif part == 'week' -%}
        DATE_FORMAT(DATE_SUB({{value}}, INTERVAL WEEKDAY({{value}}) DAY), '%Y-%m-%d 00:00:00')
    {%- elif part == 'day' -%}
        {%- if datatype | upper == 'DATE' -%}
            {{value}}
        {%- else -%}
            DATE_FORMAT({{value}}, '%Y-%m-%d 00:00:00')
        {%- endif -%}
    {%- elif part == 'hour' -%}
        DATE_FORMAT({{value}}, '%Y-%m-%d %H:00:00')
    {%- elif part == 'millisecond' -%}
        DATE_FORMAT({{value}}, '%Y-%m-%d %H:%i:%s.%f')
    {%- else -%}
        DATE_FORMAT({{value}}, '%Y-%m-%d 00:00:00')
    {%- endif -%}
{% endmacro %}


